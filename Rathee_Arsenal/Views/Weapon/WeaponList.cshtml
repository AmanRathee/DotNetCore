@model WeaponListVM
<div>
    <h2>
        <u>
            @ViewBag.Heading
            <hr />
        </u>
    </h2>
</div>

@{
    <div class="row">

        @foreach (Weapon weapon in Model.Weapons)
        {
        <div>
           @Html.Partial("WeaponSummary",weapon)
        </div>
        }
    </div>

}
